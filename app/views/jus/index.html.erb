<p id="notice"><%= notice %></p>

<h1>Jus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jus.each do |ju| %>
      <tr>
        <td><%= link_to 'Show', ju %></td>
        <td><%= link_to 'Edit', edit_ju_path(ju) %></td>
        <td><%= link_to 'Destroy', ju, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

